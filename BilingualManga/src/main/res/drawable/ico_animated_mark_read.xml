<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="52dp"
            android:height="52dp"
            android:viewportWidth="52"
            android:viewportHeight="52">
            <group
                android:name="page"
                android:pivotX="20"
                android:pivotY="20">
                <path
                    android:name="page_mark"
                    android:pathData="M 28.6 11.4 L 33.7 11.4 C 34.3 11.4 34.8 10.9 34.8 10.3 C 34.8 10 34.7 9.8 34.5 9.5 L 27.3 2.4 C 27.1 2.2 26.9 2.1 26.6 2.1 C 26 2.1 25.5 2.6 25.5 3.2 L 25.5 8.3 C 25.5 10 26.9 11.4 28.6 11.4 Z"
                    android:fillColor="?attr/colorSecondary"
                    android:strokeWidth="1"/>
                <path
                    android:name="page_sheet"
                    android:pathData="M 24.6 40.6 C 24.6 33.8 28.8 28 34.8 25.7 L 34.8 17.6 C 34.8 16.7 34.1 16 33.2 16 L 25.4 16 C 22.8 16 20.7 13.9 20.7 11.4 L 20.7 3.6 C 20.8 2.8 20.1 2.1 19.2 2.1 L 6.8 2.1 C 4.2 2.1 2.1 4.2 2.1 6.7 L 2.1 36.1 C 2.1 38.7 4.2 40.7 6.8 40.7 L 24.6 40.7 L 24.6 40.6 Z"
                    android:fillColor="?attr/colorSecondary"
                    android:strokeWidth="1"/>
            </group>
            <group
                android:name="mark"
                android:pivotX="40"
                android:pivotY="40">
                <path
                    android:name="mark_ico"
                    android:pathData="M 48.086 32.554 L 35.288 46.409 L 27.584 38.332"
                    android:fillColor="#000000"
                    android:fillAlpha="0"
                    android:strokeColor="?attr/colorSecondary"
                    android:strokeWidth="3"
                    android:strokeLineCap="round"
                    android:strokeLineJoin="round"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="mark_ico">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:startOffset="401"
                    android:duration="399"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="trimPathStart"
                    android:startOffset="363"
                    android:duration="38"
                    android:valueFrom="1"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:duration="363"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="363"
                    android:duration="38"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="page">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="401"
                    android:duration="399"
                    android:valueFrom="0.1"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:startOffset="401"
                    android:duration="399"
                    android:valueFrom="0.1"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="358"
                    android:valueFrom="1"
                    android:valueTo="0.1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="scaleY"
                    android:duration="358"
                    android:valueFrom="1"
                    android:valueTo="0.1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
