<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="350dp"
            android:height="350dp"
            android:viewportWidth="350"
            android:viewportHeight="350">
            <group
                android:name="group"
                android:pivotX="150"
                android:pivotY="150"
                android:translateX="5"
                android:rotation="90">
                <group
                    android:name="leftarrow"
                    android:translateX="300"
                    android:translateY="20"
                    android:scaleX="-1">
                    <path
                        android:name="arrowleft"
                        android:pathData="M 47.89 218.23 C 18.3 188.64 0 147.77 0 102.62 C 0 63.76 13.56 28.06 36.21 0"
                        android:fillAlpha="0"
                        android:strokeColor="?attr/colorSecondary"
                        android:strokeWidth="20"/>
                    <group
                        android:name="tipleft"
                        android:translateX="-5">
                        <path
                            android:name="lefttip"
                            android:pathData="M 0 5.61 C 0 5.61 43.46 0 43.46 0 C 43.46 0 52.02 43.37 52.02 43.37"
                            android:fillAlpha="0"
                            android:strokeColor="?attr/colorSecondary"
                            android:strokeWidth="20"/>
                    </group>
                </group>
                <group
                    android:name="cellphone"
                    android:translateX="170"
                    android:translateY="130">
                    <path
                        android:name="phone"
                        android:pathData="M 62 -87.5 C 62 -87.5 62 87.5 62 87.5 C 62 98.54 53.04 107.5 42 107.5 C 42 107.5 -42 107.5 -42 107.5 C -53.04 107.5 -62 98.54 -62 87.5 C -62 87.5 -62 -87.5 -62 -87.5 C -62 -98.54 -53.04 -107.5 -42 -107.5 C -42 -107.5 42 -107.5 42 -107.5 C 53.04 -107.5 62 -98.54 62 -87.5"
                        android:fillColor="?attr/colorSecondary"
                        android:fillAlpha="0"
                        android:strokeColor="?attr/colorSecondary"
                        android:strokeWidth="20"/>
                    <group
                        android:name="btn"
                        android:translateY="70">
                        <path
                            android:name="button"
                            android:pathData="M 0 -10.5 C 5.79 -10.5 10.5 -5.79 10.5 0 C 10.5 5.79 5.79 10.5 0 10.5 C -5.79 10.5 -10.5 5.79 -10.5 0 C -10.5 -5.79 -5.79 -10.5 0 -10.5"
                            android:fillColor="?attr/colorSecondary"
                            android:strokeColor="?attr/colorSecondary"
                            android:strokeWidth="10"/>
                    </group>
                </group>
                <group
                    android:name="rightarrow"
                    android:translateX="70"
                    android:translateY="20"
                    android:scaleX="-1">
                    <path
                        android:name="arrowright"
                        android:pathData="M 0 0 C 29.59 29.59 47.89 70.46 47.89 115.6 C 47.89 160.75 29.59 201.62 0 231.21"
                        android:fillAlpha="0"
                        android:strokeColor="?attr/colorSecondary"
                        android:strokeWidth="20"
                        android:strokeLineCap="round"
                        android:strokeLineJoin="round"/>
                    <group
                        android:name="tipright"
                        android:translateX="-15"
                        android:translateY="190">
                        <path
                            android:name="righttip"
                            android:pathData="M 53.69 35.34 C 53.69 35.34 10.54 42.93 10.54 42.93 C 10.54 42.93 0 0 0 0"
                            android:fillAlpha="0"
                            android:strokeColor="?attr/colorSecondary"
                            android:strokeWidth="20"/>
                    </group>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="2"
                    android:duration="465"
                    android:valueFrom="90"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="16"
                    android:duration="416"
                    android:valueFrom="0"
                    android:valueTo="30"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="666"
                    android:duration="399"
                    android:valueFrom="30"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="653"
                    android:duration="448"
                    android:valueFrom="0"
                    android:valueTo="90"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="1376"
                    android:duration="434"
                    android:valueFrom="0"
                    android:valueTo="30"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="rotation"
                    android:startOffset="1359"
                    android:duration="483"
                    android:valueFrom="90"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="arrowleft">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="431"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1101"
                    android:duration="221"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1779"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="lefttip">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="432"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1101"
                    android:duration="221"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1779"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="righttip">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="432"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1101"
                    android:duration="221"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1779"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="arrowright">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="432"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1101"
                    android:duration="221"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
                <objectAnimator
                    android:propertyName="strokeAlpha"
                    android:startOffset="1779"
                    android:duration="221"
                    android:valueFrom="1"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/fast_out_slow_in"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
