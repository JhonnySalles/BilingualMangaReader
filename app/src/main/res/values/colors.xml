        colorPrimary, colorOnPrimary = Text color white/green

        colorSecondary, colorOnSecondary = Title, Icon, Hint

        colorOutline = Divider

        colorTertiary = Ripple

        colorSurface = Background system
        colorOnSurface = Background accent, background text box
        colorOnBackground = Background accent
        colorOnSurfaceInverse = Background card

        colorSurfaceVariant, colorOnSurfaceVariant =

        colorOnTertiary, colorTertiaryContainer = Card text and background semi transparent


        colorPrimaryContainer, colorOnPrimaryContainer = Button, ProgressBar, selected page
        colorSecondaryContainer, colorOnSecondaryContainer = Floating popup, button and background
        colorOnTertiaryContainer = NotLinked background semi transparent

        colorPrimaryInverse = text white
        colorSurfaceInverse = touch color

        colorError, colorOnError = erros, excluded
        colorErrorContainer, colorOnErrorContainer = erros acent
